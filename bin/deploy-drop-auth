#!/usr/bin/env bash
set -ex

export ETH_FROM=${ETH_FROM:-$(seth rpc eth_coinbase)}

# drop ownership of a deployed system
objects="$SAI_VOX
$SAI_TUB
$SAI_TAP
$SAI_TOP
$SAI_SAI
$SAI_SIN
$SAI_SKR
$SAI_DAD
$SAI_MOM
"

export SETH_ASYNC=yes

for object in $objects; do
    seth send $object 'setOwner(address)' "0x0000000000000000000000000000000000000000"
done
